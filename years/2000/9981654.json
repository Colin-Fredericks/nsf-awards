 "awd_titl_txt": "Does Lack of Herbivory and Disease Explain the Success of an Alien Plant Species?: Experimental Tests",
 "cfda_num": "47.074",
 "org_code": "08010208",
 "po_phone": null,
 "po_email": "",
 "po_sign_block_name": "Susan Mazer",
 "awd_eff_date": "2000-05-01",
